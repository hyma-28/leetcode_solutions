class Solution:
    def coinChange(self, coins: List[int], amount: int) -> int:
        if amount<1:
            return 0
        dp=[0]+[float('inf')]*amount
        for coin in coins:
            for i in range(coin,amount+1):
                dp[i]=min(dp[i],1+dp[i-coin])
        if dp[amount]==float('inf'):
            return -1
        return dp[amount]
